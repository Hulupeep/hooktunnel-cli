{
  "name": "hooktunnel-cli",
  "version": "0.1.0",
  "description": "CLI for HookTunnel - Forward webhooks to localhost",
  "type": "module",
  "bin": {
    "hooktunnel": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:contracts": "vitest run tests/contracts/",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "webhook",
    "tunnel",
    "ngrok",
    "development",
    "debugging"
  ],
  "author": "HookTunnel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hulupeep/hooktunnel-cli.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "ora": "^8.0.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
